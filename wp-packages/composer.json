{
	"name": "aztecweb/aztlan",
	"repositories": [{
		"type": "composer",
		"url": "https://wpackagist.org"
	}],
	"require": {
		"johnpbloch/wordpress": "^5.2",
		"wpackagist-plugin/elementor": "^2.9.6",
		"wpackagist-plugin/ocean-extra": "^1.6.0",
		"wpackagist-plugin/wpforms-lite": "^1.5.9.5"
	},
	"extra": {
		"wordpress-install-dir": "../public/wp",
		"installer-paths": {
			"../public/packages/plugins/{$name}/": [
				"type:wordpress-plugin"
			],
			"../public/packages/themes/{$name}/": [
				"type:wordpress-theme"
			]
		}
	}
}
